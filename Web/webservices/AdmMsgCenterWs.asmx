        private AdmMsgCenter86 PrepAdmMsgCenterData(SerializableDictionary<string, string> mst, List<SerializableDictionary<string, string>> dtl, bool bAdd)
        {
            AdmMsgCenter86 ds = new AdmMsgCenter86();
            DataRow dr = ds.Tables["AdmMsgCenter"].NewRow();
            DataRow drType = ds.Tables["AdmMsgCenter"].NewRow();
            DataRow drDisp = ds.Tables["AdmMsgCenter"].NewRow();

            if (bAdd) { dr["MsgId146"] = string.Empty; } else { dr["MsgId146"] = mst["MsgId146"]; }
            drType["MsgId146"] = "Numeric"; drDisp["MsgId146"] = "TextBox";
            try { dr["MsgTypeCd146"] = mst["MsgTypeCd146"]; } catch { }
            drType["MsgTypeCd146"] = "Char"; drDisp["MsgTypeCd146"] = "DropDownList";
            try { dr["MsgSource146"] = (mst["MsgSource146"] ?? "").Trim().Left(50); } catch { }
            drType["MsgSource146"] = "VarChar"; drDisp["MsgSource146"] = "TextBox";

            if (dtl != null)
            {
                ds.Tables["AdmMsgCenterDef"].Rows.Add(MakeTypRow(ds.Tables["AdmMsgCenterDef"].NewRow()));
                ds.Tables["AdmMsgCenterDef"].Rows.Add(MakeDisRow(ds.Tables["AdmMsgCenterDef"].NewRow()));
                if (bAdd)
                {
                    foreach (var drv in dtl)
                    {
                        ds.Tables["AdmMsgCenterAdd"].Rows.Add(MakeColRow(ds.Tables["AdmMsgCenterAdd"].NewRow(), drv, mst["MsgId146"], true));
                    }
                }
                else
                {
                    var dtlUpd = from r in dtl where !string.IsNullOrEmpty((r["MsgCenterId147"] ?? "").ToString()) && (r.ContainsKey("_mode") ? r["_mode"] : "") != "delete" select r;
                    foreach (var drv in dtlUpd)
                    {
                        ds.Tables["AdmMsgCenterUpd"].Rows.Add(MakeColRow(ds.Tables["AdmMsgCenterUpd"].NewRow(), drv, mst["MsgId146"], false));
                    }
                    var dtlAdd = from r in dtl.AsEnumerable() where string.IsNullOrEmpty(r["MsgCenterId147"]) select r;
                    foreach (var drv in dtlAdd)
                    {
                        ds.Tables["AdmMsgCenterAdd"].Rows.Add(MakeColRow(ds.Tables["AdmMsgCenterAdd"].NewRow(), drv, mst["MsgId146"], true));
                    }
                    var dtlDel = from r in dtl.AsEnumerable() where !string.IsNullOrEmpty((r["MsgCenterId147"] ?? "").ToString()) && (r.ContainsKey("_mode") ? r["_mode"] : "") == "delete" select r;
                    foreach (var drv in dtlDel)
                    {
                        ds.Tables["AdmMsgCenterDel"].Rows.Add(MakeColRow(ds.Tables["AdmMsgCenterDel"].NewRow(), drv, mst["MsgId146"], false));
                    }
                }
            }
            ds.Tables["AdmMsgCenter"].Rows.Add(dr); ds.Tables["AdmMsgCenter"].Rows.Add(drType); ds.Tables["AdmMsgCenter"].Rows.Add(drDisp);

            return ds;
        }

