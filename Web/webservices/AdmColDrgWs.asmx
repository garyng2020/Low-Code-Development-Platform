                        ds.Tables["AdmColDrgUpd"].Rows.Add(MakeColRow(ds.Tables["AdmColDrgUpd"].NewRow(), drv, mst["ScreenObjId14"], false));
                    }
                    var dtlAdd = from r in dtl.AsEnumerable() where string.IsNullOrEmpty(r["ScreenObjId14"]) select r;
                    foreach (var drv in dtlAdd)
                    {
                        ds.Tables["AdmColDrgAdd"].Rows.Add(MakeColRow(ds.Tables["AdmColDrgAdd"].NewRow(), drv, mst["ScreenObjId14"], true));
                    }
                    var dtlDel = from r in dtl.AsEnumerable() where !string.IsNullOrEmpty((r["ScreenObjId14"] ?? "").ToString()) && (r.ContainsKey("_mode") ? r["_mode"] : "") == "delete" select r;
                    foreach (var drv in dtlDel)
