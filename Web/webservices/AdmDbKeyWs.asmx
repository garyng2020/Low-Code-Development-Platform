        private AdmDbKey15 PrepAdmDbKeyData(SerializableDictionary<string, string> mst, List<SerializableDictionary<string, string>> dtl, bool bAdd)
        {
            AdmDbKey15 ds = new AdmDbKey15();
            DataRow dr = ds.Tables["AdmDbKey"].NewRow();
            DataRow drType = ds.Tables["AdmDbKey"].NewRow();
            DataRow drDisp = ds.Tables["AdmDbKey"].NewRow();

            if (bAdd) { dr["KeyId20"] = string.Empty; } else { dr["KeyId20"] = mst["KeyId20"]; }
            drType["KeyId20"] = "Numeric"; drDisp["KeyId20"] = "TextBox";
            try { dr["KeyName20"] = (mst["KeyName20"] ?? "").Trim().Left(50); } catch { }
            drType["KeyName20"] = "VarChar"; drDisp["KeyName20"] = "TextBox";
            try { dr["TableId20"] = mst["TableId20"]; } catch { }
            drType["TableId20"] = "Numeric"; drDisp["TableId20"] = "AutoComplete";
            try { dr["ColumnId20"] = mst["ColumnId20"]; } catch { }
            drType["ColumnId20"] = "Numeric"; drDisp["ColumnId20"] = "AutoComplete";
            try { dr["RefTableId20"] = mst["RefTableId20"]; } catch { }
            drType["RefTableId20"] = "Numeric"; drDisp["RefTableId20"] = "AutoComplete";
            try { dr["RefColumnId20"] = mst["RefColumnId20"]; } catch { }
            drType["RefColumnId20"] = "Numeric"; drDisp["RefColumnId20"] = "AutoComplete";

            if (dtl != null)
            {
                ds.Tables["AdmDbKeyDef"].Rows.Add(MakeTypRow(ds.Tables["AdmDbKeyDef"].NewRow()));
                ds.Tables["AdmDbKeyDef"].Rows.Add(MakeDisRow(ds.Tables["AdmDbKeyDef"].NewRow()));
                if (bAdd)
                {
                    foreach (var drv in dtl)
                    {
                        ds.Tables["AdmDbKeyAdd"].Rows.Add(MakeColRow(ds.Tables["AdmDbKeyAdd"].NewRow(), drv, mst["KeyId20"], true));
                    }
                }
                else
                {
                    var dtlUpd = from r in dtl where !string.IsNullOrEmpty((r[""] ?? "").ToString()) && (r.ContainsKey("_mode") ? r["_mode"] : "") != "delete" select r;
                    foreach (var drv in dtlUpd)
                    {
                        ds.Tables["AdmDbKeyUpd"].Rows.Add(MakeColRow(ds.Tables["AdmDbKeyUpd"].NewRow(), drv, mst["KeyId20"], false));
                    }
                    var dtlAdd = from r in dtl.AsEnumerable() where string.IsNullOrEmpty(r[""]) select r;
                    foreach (var drv in dtlAdd)
                    {
                        ds.Tables["AdmDbKeyAdd"].Rows.Add(MakeColRow(ds.Tables["AdmDbKeyAdd"].NewRow(), drv, mst["KeyId20"], true));
                    }
                    var dtlDel = from r in dtl.AsEnumerable() where !string.IsNullOrEmpty((r[""] ?? "").ToString()) && (r.ContainsKey("_mode") ? r["_mode"] : "") == "delete" select r;
                    foreach (var drv in dtlDel)
                    {
                        ds.Tables["AdmDbKeyDel"].Rows.Add(MakeColRow(ds.Tables["AdmDbKeyDel"].NewRow(), drv, mst["KeyId20"], false));
                    }
                }
            }
            ds.Tables["AdmDbKey"].Rows.Add(dr); ds.Tables["AdmDbKey"].Rows.Add(drType); ds.Tables["AdmDbKey"].Rows.Add(drDisp);

            return ds;
        }

