                    }
                    var dtlAdd = from r in dtl.AsEnumerable() where string.IsNullOrEmpty(r["ButtonHlpId116"]) select r;
                    foreach (var drv in dtlAdd)
                    {
                        ds.Tables["AdmButtonHlpAdd"].Rows.Add(MakeColRow(ds.Tables["AdmButtonHlpAdd"].NewRow(), drv, mst["ButtonHlpId116"], true));
                    }
                    var dtlDel = from r in dtl.AsEnumerable() where !string.IsNullOrEmpty((r["ButtonHlpId116"] ?? "").ToString()) && (r.ContainsKey("_mode") ? r["_mode"] : "") == "delete" select r;
